//senha e repita a solicitação até que a senha
//seja digitada corretamente.
//Crie a senha que você quiser!
programa {
  funcao inicio() {
    inteiro senha = 123

    faca
   {
    limpa()
    escreva("Digite a senha: ")
    leia(senha)
    }

   enquanto (senha != 123)
   escreva("Senha Correta")
    
    
  
  }
}
