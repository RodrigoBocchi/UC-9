programa
{
	
	funcao inicio()
	{
		inteiro num_1, num_2, num_3, resultado

		escreva ("informe o primeiro Numero: ")
		leia(num_1)
		escreva ("informe o segundo numero: ")
		leia(num_2)
		escreva ("informe o terceiro numero: ")
		leia(num_3)

		resultado = (num_1 + num_2 + num_3) /3
		escreva("O resultado da soma dos 3 é: " + resultado)
	}
}
