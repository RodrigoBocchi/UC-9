programa {
  funcao inicio() {
    inteiro num
    escreva("Digite o numero para verificar se é maior que 0: ")
    leia(num)
    se (num > 0)
    {
    escreva (num, "é maior que 0")
    }
    senao
    {
    escreva (num, "é menor que 0")
    }
      se (num < 0)
      {
      escreva (num, "é menor que 0")
      }
      senao
      {
      escreva (num, "é igual a 0")
      }
  }
}
